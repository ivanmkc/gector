    "version": "0.2.0",
    "configurations": [
        {
            "name": "Grammar Server",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--reload"
            ]
        },
        {
            "name": "Build docker image",
            "request": "launch",
            "command": "gcloud builds submit --tag=us.gcr.io/chatbot-299615/chatbot-grammar:latest --timeout=3600",
            "type": "node-terminal",
        },
        {
            "name": "Deploy docker image",
            "request": "launch",
            "command": "gcloud run deploy chatbot-grammar --image=us.gcr.io/chatbot-299615/chatbot-rte:latest --platform=managed --concurrency=1 --set-env-vars=MKL_NUM_THREADS=2,OMP_NUM_THREADS=2,NUMEXPR_NUM_THREADS=2 --cpu=2 --memory=3G --allow-unauthenticated",
            "type": "node-terminal",
        },
        {
            "name": "Show logs",
            "request": "launch",
            "command": "gcloud app logs tail -s default",
            "type": "node-terminal",
        }
    ]
}